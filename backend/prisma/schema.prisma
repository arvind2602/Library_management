generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int       @id @default(autoincrement())
  username   String    @unique
  password   String    @default("")
  role       String    
  created_at DateTime  @default(now())
  updated_at DateTime? @updatedAt
}
